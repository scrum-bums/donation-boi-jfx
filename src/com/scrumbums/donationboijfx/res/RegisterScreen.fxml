<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.scrumbums.donationboijfx.main.controller.RegisterScreen">
   <children>
      <VBox layoutX="181.0" layoutY="91.0" prefHeight="218.0" prefWidth="239.0">
         <children>
            <TextField promptText="Name" fx:id="nameField">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField layoutX="10.0" layoutY="10.0" promptText="Email" fx:id="emailField">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField layoutX="10.0" layoutY="10.0" promptText="Username" fx:id="usernameField">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextField>
            <PasswordField promptText="Password" fx:id="passwordField">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </PasswordField>
            <Spinner prefHeight="26.0" prefWidth="241.0" fx:id="roleSpinner">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Spinner>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <children>
                  <Button mnemonicParsing="false" text="Register" fx:id="registerButton"/>
                  <Button mnemonicParsing="false" text="Cancel" fx:id="cancelButton"/>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
